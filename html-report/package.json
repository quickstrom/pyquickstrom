{
  "name": "html-report",
  "version": "0.1.0",
  "scripts": {
    "build": "mkdir -p dist && browserify src/index.tsx -p [ tsify ] > dist/index.js && cp src/*.{css,html} dist/",
    "dev": "yarn build && cp -r dev/* dist/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "preact": "^10.5.7"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "tsify": "^5.0.4",
    "typescript": "^4.1.2"
  }
}
